  "tangle": {
    "milestoneTimeout": "30s",
    "maxDeltaBlockYoungestConeRootIndexToCMI": 2,
    "maxDeltaBlockOldestConeRootIndexToCMI": 7,
    "whiteFlagParentsSolidTimeout": "2s"
