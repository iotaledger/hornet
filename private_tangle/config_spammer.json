      "sendBasicOutput": true,
      "collectBasicOutput": true,
      "createAlias": true,
      "destroyAlias": true,
      "createFoundry": true,
      "destroyFoundry": true,
      "mintNativeToken": true,
      "meltNativeToken": true,
      "createNFT": true,
      "destroyNFT": true
