{
  "useProfile": "auto",
  "httpAPI": {
    "basicAuth": {
      "enabled": false,
      "username": "",
      "passwordHash": "",
      "passwordSalt": ""
    },
    "excludeHealthCheckFromAuth": false,
    "permitRemoteAccess": [
      "getNodeInfo",
      "getBalances",
      "checkConsistency",
      "getTipInfo",
      "getTransactionsToApprove",
      "getInclusionStates",
      "getNodeAPIConfiguration",
      "broadcastTransactions",
      "findTransactions",
      "storeTransactions",
      "getTrytes"
    ],
    "permittedRoutes": [
      "healthz"
    ],
    "whitelistedAddresses": [],
    "bindAddress": "0.0.0.0:14265",
    "limits": {
      "bodyLengthBytes": 1000000,
      "findTransactions": 1000,
      "getTrytes": 1000,
      "requestsList": 1000
    }
  },
  "dashboard": {
    "bindAddress": "localhost:8081",
    "theme": "default",
    "dev": false,
    "basicAuth": {
      "enabled": false,
      "username": "",
      "passwordHash": "",
      "passwordSalt": ""
    }
  },
  "db": {
    "path": "alphanetdb"
  },
  "snapshots": {
    "loadType": "global",
    "local": {
      "depth": 50,
      "intervalSynced": 200,
      "intervalUnsynced": 1000,
      "path": "snapshots/alphanet/export.bin",
      "downloadURLs": []
    },
    "global": {
      "path": "snapshotAlphanet.txt",
      "index": 0
    },
    "pruning": {
      "enabled": true,
      "delay": 60480
    }
  },
  "coordinator": {
    "publicKey": "ed3c3f1a319ff4e909cf2771d79fece0ac9bd9fd2ee49ea6c0885c9cb3b1248c",
    "mwm": 1,
    "stateFilePath": "coordinator.state",
    "intervalSeconds": 10,
    "checkpoints": {
      "maxTrackedMessages": 10000
    },
    "tipsel": {
      "minHeaviestBranchUnconfirmedMessagesThreshold": 20,
      "maxHeaviestBranchTipsPerCheckpoint": 10,
      "randomTipsPerCheckpoint": 2,
      "heaviestBranchSelectionDeadlineMilliseconds": 100
    }
  },
  "network": {
    "preferIPv6": false,
    "gossip": {
      "bindAddress": "0.0.0.0:15600",
      "reconnectAttemptIntervalSeconds": 60
    },
    "autopeering": {
      "bindAddress": "0.0.0.0:14626",
      "runAsEntryNode": false,
      "entryNodes": [],
      "seed": ""
    }
  },
  "tipsel": {
    "maxDeltaMsgYoungestConeRootIndexToLSMI": 2,
    "maxDeltaMsgOldestConeRootIndexToLSMI": 7,
    "belowMaxDepth": 15,
    "nonLazy": {
      "retentionRulesTipsLimit": 100,
      "maxReferencedTipAgeSeconds": 3,
      "maxChildren": 2,
      "spammerTipsThreshold": 0
    },
    "semiLazy": {
      "retentionRulesTipsLimit": 20,
      "maxReferencedTipAgeSeconds": 3,
      "maxChildren": 2,
      "spammerTipsThreshold": 30
    }
  },
  "node": {
    "alias": "",
    "showAliasInGetNodeInfo": false,
    "disablePlugins": ["Autopeering"],
    "enablePlugins": []
  },
  "logger": {
    "level": "debug",
    "disableCaller": true,
    "encoding": "console",
    "outputPaths": [
      "stdout"
    ]
  },
  "warpsync": {
    "advancementRange": 50
  },
  "spammer": {
    "message": "Binary is the future.",
    "index": "HORNET Spammer",
    "indexSemiLazy": "HORNET Spammer Semi-Lazy",
    "cpuMaxUsage": 0.8,
    "mpsRateLimit": 0.0,
    "workers": 0,
    "autostart": false
  },
  "mqtt": {
    "config": "mqtt_config.json"
  },
  "zmq": {
    "bindAddress": "localhost:5556"
  },
  "profiling": {
    "bindAddress": "localhost:6060"
  },
  "prometheus": {
    "bindAddress": "localhost:9311",
    "goMetrics": false,
    "processMetrics": false,
    "promhttpMetrics": false
  }
}
