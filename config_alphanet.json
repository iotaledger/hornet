{
  "useProfile": "auto",
  "restAPI": {
    "basicAuth": {
      "enabled": false,
      "username": "",
      "passwordHash": "",
      "passwordSalt": ""
    },
    "excludeHealthCheckFromAuth": false,
    "permittedRoutes": [
      "/health",
      "/api/v1/info",
      "/api/v1/tips",
      "/api/v1/messages/:messageID",
      "/api/v1/messages/:messageID/metadata",
      "/api/v1/messages/:messageID/raw",
      "/api/v1/messages/:messageID/children",
      "/api/v1/messages",
      "/api/v1/milestones/:milestoneIndex",
      "/api/v1/outputs/:outputID",
      "/api/v1/addresses/:address",
      "/api/v1/addresses/:address/outputs",
      "/api/v1/peers/:peerID",
      "/api/v1/peers"
    ],
    "whitelistedAddresses": ["127.0.0.1", "::1"],
    "bindAddress": "0.0.0.0:14265",
    "limits": {
      "bodyLength": "1M",
      "maxResults": 1000
    }
  },
  "dashboard": {
    "bindAddress": "localhost:8081",
    "theme": "default",
    "dev": false,
    "basicAuth": {
      "enabled": false,
      "username": "",
      "passwordHash": "",
      "passwordSalt": ""
    }
  },
  "db": {
    "path": "alphanetdb"
  },
  "snapshots": {
    "depth": 50,
    "intervalSynced": 200,
    "intervalUnsynced": 1000,
    "path": "snapshots/alphanet/export.bin",
    "downloadURLs": []
  },
  "pruning": {
    "enabled": true,
    "delay": 500
  },
  "coordinator": {
    "publicKeyRanges": [
      {
        "key": "ed3c3f1a319ff4e909cf2771d79fece0ac9bd9fd2ee49ea6c0885c9cb3b1248c",
        "start": 0,
        "end": 0
      },
      {
        "key": "f6752f5f46a53364e2ee9c4d662d762a81efd51010282a75cd6bd03f28ef349c",
        "start": 0,
        "end": 0
      }
    ],
    "mwm": 1,
    "stateFilePath": "coordinator.state",
    "intervalSeconds": 10,
    "checkpoints": {
      "maxTrackedMessages": 10000
    },
    "tipsel": {
      "minHeaviestBranchUnreferencedMessagesThreshold": 20,
      "maxHeaviestBranchTipsPerCheckpoint": 10,
      "randomTipsPerCheckpoint": 2,
      "heaviestBranchSelectionDeadlineMilliseconds": 100
    }
  },
  "network": {
    "preferIPv6": false,
    "gossip": {
      "bindAddress": "0.0.0.0:15600",
      "reconnectAttemptIntervalSeconds": 60
    },
    "autopeering": {
      "bindAddress": "0.0.0.0:14626",
      "runAsEntryNode": false,
      "entryNodes": [],
      "seed": ""
    }
  },
  "tipsel": {
    "maxDeltaMsgYoungestConeRootIndexToLSMI": 2,
    "maxDeltaMsgOldestConeRootIndexToLSMI": 7,
    "belowMaxDepth": 15,
    "nonLazy": {
      "retentionRulesTipsLimit": 100,
      "maxReferencedTipAgeSeconds": 3,
      "maxChildren": 2,
      "spammerTipsThreshold": 0
    },
    "semiLazy": {
      "retentionRulesTipsLimit": 20,
      "maxReferencedTipAgeSeconds": 3,
      "maxChildren": 2,
      "spammerTipsThreshold": 30
    }
  },
  "node": {
    "alias": "",
    "enableProofOfWork": false,
    "disablePlugins": ["Autopeering"],
    "enablePlugins": ["Spammer", "Coordinator"]
  },
  "logger": {
    "level": "debug",
    "disableCaller": true,
    "encoding": "console",
    "outputPaths": [
      "stdout"
    ]
  },
  "warpsync": {
    "advancementRange": 50
  },
  "spammer": {
    "message": "Binary is the future.",
    "index": "HORNET Spammer",
    "indexSemiLazy": "HORNET Spammer Semi-Lazy",
    "cpuMaxUsage": 0.8,
    "mpsRateLimit": 5.0,
    "workers": 0,
    "autostart": true
  },
  "mqtt": {
    "config": "mqtt_config.json"
  },
  "zmq": {
    "bindAddress": "localhost:5556"
  },
  "profiling": {
    "bindAddress": "localhost:6060"
  },
  "prometheus": {
    "bindAddress": "localhost:9311",
    "goMetrics": false,
    "processMetrics": false,
    "promhttpMetrics": false
  }
}
