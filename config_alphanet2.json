{
  "useProfile": "debug",
  "restAPI": {
    "basicAuth": {
      "enabled": false,
      "username": "",
      "passwordHash": "",
      "passwordSalt": ""
    },
    "excludeHealthCheckFromAuth": false,
    "permittedRoutes": [
      "health"
    ],
    "whitelistedAddresses": [],
    "bindAddress": "0.0.0.0:14268",
    "limits": {
      "bodyLength": "1M",
      "maxResults": 1000
    }
  },
  "dashboard": {
    "bindAddress": "localhost:8082",
    "theme": "default",
    "dev": false,
    "basicAuth": {
      "enabled": false,
      "username": "",
      "passwordHash": "",
      "passwordSalt": ""
    }
  },
  "db": {
    "path": "alphanetdb2"
  },
  "snapshots": {
    "depth": 50,
    "intervalSynced": 200,
    "intervalUnsynced": 1000,
    "path": "snapshots/alphanet/export.bin",
    "downloadURLs": []
  },
  "pruning": {
    "enabled": true,
    "delay": 500
  },
  "coordinator": {
    "publicKey": "ed3c3f1a319ff4e909cf2771d79fece0ac9bd9fd2ee49ea6c0885c9cb3b1248c",
    "mwm": 1,
    "stateFilePath": "coordinator.state",
    "intervalSeconds": 10,
    "checkpoints": {
      "maxTrackedMessages": 10000
    },
    "tipsel": {
      "minHeaviestBranchUnreferencedMessagesThreshold": 20,
      "maxHeaviestBranchTipsPerCheckpoint": 10,
      "randomTipsPerCheckpoint": 2,
      "heaviestBranchSelectionDeadlineMilliseconds": 100
    }
  },
  "network": {
    "preferIPv6": false,
    "gossip": {
      "bindAddress": "0.0.0.0:15601",
      "reconnectAttemptIntervalSeconds": 60
    },
    "autopeering": {
      "bindAddress": "0.0.0.0:14627",
      "runAsEntryNode": false,
      "entryNodes": [],
      "seed": ""
    }
  },
  "tipsel": {
    "maxDeltaMsgYoungestConeRootIndexToLSMI": 2,
    "maxDeltaMsgOldestConeRootIndexToLSMI": 7,
    "belowMaxDepth": 15,
    "nonLazy": {
      "retentionRulesTipsLimit": 100,
      "maxReferencedTipAgeSeconds": 3,
      "maxChildren": 2,
      "spammerTipsThreshold": 0
    },
    "semiLazy": {
      "retentionRulesTipsLimit": 20,
      "maxReferencedTipAgeSeconds": 3,
      "maxChildren": 2,
      "spammerTipsThreshold": 30
    }
  },
  "node": {
    "alias": "",
    "enableProofOfWork": false,
    "disablePlugins": ["Autopeering"],
    "enablePlugins": ["Spammer"]
  },
  "logger": {
    "level": "debug",
    "disableCaller": true,
    "encoding": "console",
    "outputPaths": [
      "stdout"
    ]
  },
  "warpsync": {
    "advancementRange": 50
  },
  "spammer": {
    "message": "Binary is the future.",
    "index": "HORNET Spammer",
    "indexSemiLazy": "HORNET Spammer Semi-Lazy",
    "cpuMaxUsage": 0.8,
    "mpsRateLimit": 5.0,
    "workers": 0,
    "autostart": true
  },
  "mqtt": {
    "config": "mqtt_config.json"
  },
  "zmq": {
    "bindAddress": "localhost:5557"
  },
  "profiling": {
    "bindAddress": "localhost:6061"
  },
  "prometheus": {
    "bindAddress": "localhost:9312",
    "goMetrics": false,
    "processMetrics": false,
    "promhttpMetrics": false
  }
